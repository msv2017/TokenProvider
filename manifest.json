{
  "manifest_version": 2,
  "name": "Token Provider",
  "short_name": "TokenProvider",
  "description": "",
  "version": "1.0.0",
  "author": "Sergii Martianov",
  "minimum_chrome_version": "38",
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "32": "assets/icon.png"
  },
  "background": {
    "page": "backend.html"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "web_accessible_resources": [
    "script.js"
  ]
}